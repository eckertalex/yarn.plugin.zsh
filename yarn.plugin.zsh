#!/bin/sh

alias y="yarn"

# Dependency management
alias yi="yarn install"
alias yl="yarn list"
alias yout="yarn outdated"
alias ya="yarn add"
alias yad="yarn add --dev"
alias yrm="yarn remove"

# Development
alias yst="yarn start"
alias yd="yarn dev"
alias yb="yarn build"
alias ylt="yarn lint"

# Testing
alias yt="yarn test"
alias ytc="yarn test --coverage"

